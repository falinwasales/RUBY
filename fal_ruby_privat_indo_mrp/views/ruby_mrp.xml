<?xml version="1.0"?>
<odoo>
	<record id="fal_ruby_mrp_product_category" model="ir.ui.view">
        <field name="name">fal_ruby_mrp_product_category</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="fal_product_cateq" string="Product Category"/>
            </xpath>
        </field>    
    </record>
</odoo>
